<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>
            getAdmins
        </servlet-name>
        <servlet-class>
            com.nacl.servlet.getAdmins
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getAdmins</servlet-name>
        <url-pattern>
            /getAdmins
        </url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>
            getAdminById
        </servlet-name>
        <servlet-class>
            com.nacl.servlet.getAdminById
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>
            getAdminById
        </servlet-name>
        <url-pattern>
            /getAdminById
        </url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>
            addAdmin
        </servlet-name>
        <servlet-class>
            com.nacl.servlet.addAdmin
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addAdmin</servlet-name>
        <url-pattern>/addAdmin</url-pattern>
    </servlet-mapping>
</web-app>